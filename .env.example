
# ============================================================================
# COFIRA - Environment Configuration Example
# ============================================================================
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_NAME=cofira
DB_USERNAME=admin
DB_PASSWORD=admin123
DB_PORT=5433

# PgAdmin Configuration
PGADMIN_EMAIL=admin@admin.com
PGADMIN_PASSWORD=admin123

# Backend Configuration
SPRING_PROFILES_ACTIVE=docker
JWT_SECRET=your_super_secret_jwt_key_change_in_production_min_256_bits
JWT_EXPIRATION_MS=86400000
SERVER_PORT=8081

# Frontend Configuration
ANGULAR_PORT=4200

# Email Configuration (Gmail SMTP)
# To use Gmail, create an App Password at https://myaccount.google.com/apppasswords
GMAIL_USERNAME=your-email@gmail.com
GMAIL_APP_PASSWORD=xxxx-xxxx-xxxx-xxxx

# Gemini API Configuration
GEMINI_API=your-gemini-api-key

# CORS Configuration
CORS_ALLOWED_ORIGINS=https://v1.cofira.app,http://localhost:4200
